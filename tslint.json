{
    "extends": ["tslint:latest", "tslint-react"],
    "rules": {
      "quotemark":[true, "single"],
      "trailing-comma": false,
      "interface-name": [true, "never-prefix"],
      "arrow-parens": false,
      "member-access": false,
      "no-trailing-whitespace": [true, "ignore-comments", "ignore-blank-lines"]
    }
  }